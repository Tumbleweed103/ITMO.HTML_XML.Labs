  xmlns:lessons CDATA #FIXED 'http://www.itmo.ru/xml/lessons'>

<!ELEMENT lessons:DateBegin (#PCDATA)>
<!ATTLIST lessons:DateBegin
  xmlns:lessons CDATA #FIXED 'http://www.itmo.ru/xml/lessons'>

<!ELEMENT lessons:DateEnd (#PCDATA)>
<!ATTLIST lessons:DateEnd
  xmlns:lessons CDATA #FIXED 'http://www.itmo.ru/xml/lessons'>

<!ELEMENT lessons:Lecturer (#PCDATA)>
<!ATTLIST lessons:Lecturer
  xmlns:lessons CDATA #FIXED 'http://www.itmo.ru/xml/lessons'>

<!ELEMENT lessons:Hours (#PCDATA)>
<!ATTLIST lessons:Hours
  xmlns:lessons CDATA #FIXED 'http://www.itmo.ru/xml/lessons'>
